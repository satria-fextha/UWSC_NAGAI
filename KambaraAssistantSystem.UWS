//____________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________
PROCEDURE ProcessTable(VslInfo)
  EXCEL = CREATEOLEOBJ("Excel.Application")
  SELECT SLCTBOX(SLCT_BTN,0,"処理一覧<#CR><#CR>対象船:" + VslInfo,_
                            "(IMP)Manifest Output",_
                            "Booking Status Report",_
                            "(IMP)Freight Charge List Output",_
                            "(EXP)Freight Charge List Output",_
                            "Actual Schedule Input",_
                            "Arrival Notice Print by PDF",_
                            "VSL Add to DoSystem",_
                            "Arrival Notece Fax",_
                            "戻る")
  CASE SLCT_1
    Excel.QUIT
    CALL KambaraManifestPrint.uws(VslInfo)
  CASE SLCT_2
    Excel.QUIT
    CALL KambaraBookingStatusReport.uws(VslInfo)
  CASE SLCT_3
    Excel.QUIT
    CALL KambaraFreightChargeListVVD.uws(VslInfo)
  CASE SLCT_4
    Excel.QUIT
    CALL KambaraExpFreightChargeListVVD.uws(VslInfo)
  CASE SLCT_5
    Excel.QUIT
    CALL KambaraActualScheduleInput.uws(VslInfo)
  CASE SLCT_6
    Excel.QUIT
    CALL KambaraArrivalPrint.uws(VslInfo)
  CASE SLCT_7
    Excel.QUIT
    CALL KambaraVSLSheetAddToDoSystem.uws(VslInfo)
  CASE SLCT_8
    Excel.QUIT
    CALL KambaraArrivalFax.uws(VslInfo)
  CASE SLCT_9
    Excel.QUIT
    ScheduleList()
  DEFAULT
    Excel.QUIT
  SELEND
FEND

