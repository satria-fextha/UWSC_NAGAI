PROCEDURE Home()
  SELECT SLCTBOX(SLCT_BTN,0,"その他","OPUSスケジュール反映",_
                                    "NABIS船社チャージリスト更新",_
                                    "戻る",_
                                    "システム終了")
  CASE SLCT_1
    CALL KambaraVesselUpdate.uws()
  CASE SLCT_2
    Call NABIS船社チャージリスト保存()
  CASE SLCT_3
    ScheduleList()
  CASE SLCT_4
    EXITEXIT
  SELEND
FEND